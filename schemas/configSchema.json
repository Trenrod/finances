{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "fixedIncome": {
      "type": "number",
      "minimum": 0
    },
    "labels": {
      "minItems": 1,
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "color": {
            "type": "string"
          }
        },
        "required": [
          "name",
          "color"
        ],
        "additionalProperties": false
      }
    }
  },
  "required": [
    "fixedIncome",
    "labels"
  ],
  "additionalProperties": false
}